{
  "name": "auction",
  "version": "1.0.0",
  "description": "Participate in the Decentraland's Auction",
  "main": "src/server.js",
  "scripts": {
    "docs": "npx jsdoc -c conf.json -r src/**/*.js",
    "lint": "npx eslint .",
    "fix": "npx eslint -f .",
    "test": "npx mocha -r babel-register -r specs/spec_utils.js specs/setup.js specs/*.spec.js",
    "watch:test": "npx mocha -r babel-register -r specs/spec_utils.js specs/setup.js specs/*.spec.js --watch",
    "build": "npm run lint && npm run test",
    "start": "babel-node src/server.js",
    "dev": "nodemon --exec npm run start"
  },
  "repository": {
    "type": "git",
    "url": "https://decentraland.digital/dApps/auction.git"
  },
  "keywords": [
    "decentraland",
    "auction",
    "dapp"
  ],
  "author": "Decentraland",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "decentraland-commons": "git+ssh://git@github.com/decentraland/commons.git#release",
    "express": "^4.16.2",
    "postgres-bytea": "^1.0.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.8.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "jsdoc": "^3.5.5",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "prettier": "^1.7.4",
    "sinon": "^4.0.1"
  }
}
